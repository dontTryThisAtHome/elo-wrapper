{
    "name": "donttrythisathome/elo-wrapper",
    "description": "EloWrapper is a wrapper around Laravel Eloquent model that allows you to create specific model actions in separated wrapper classes.",
    "type": "library",
    "keywords": ["decorator", "wrapper", "laravel 5", "eloquent", "model"],
    "license": "MIT",
    "authors": [
        {
            "name": "Constantine Derkach",
            "email": "koderkach@gmail.com",
            "role":"Developer"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "~5.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Dtth\\EloWrapper\\Tests\\": "tests/"
        }
    },
    "autoload": {
        "psr-4": {
            "Dtth\\EloWrapper\\": "src/"
        }
    }
}